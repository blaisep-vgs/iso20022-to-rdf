@base <http://example.org/> .
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix dct: <http://purl.org/dc/terms/>.
@prefix qb: <http://purl.org/linked-data/cube#>.
@prefix carml: <http://carml.taxonic.com/carml/> .
@prefix camt53: <http://example.org/cam53/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#AccountStatement> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
      a carml:Stream ;
		  carml:declaresNamespace [
			carml:namespacePrefix "cam" ;
			carml:namespaceName "urn:iso:std:iso:20022:tech:xsd:camt.053.001.04" ;
		  ] ;
	  ] ;
    rml:referenceFormulation ql:XPath ;
    rml:iterator "/cam:Document/cam:BkToCstmrStmt/cam:Stmt/cam:Ntry" ;
  ] ;

  rr:subjectMap [
    rr:template "http://data.ktk.netlabs.org/{cam:AcctSvcrRef}" ;
    rr:class qb:Observation ;
  #  rr:termType rr:IRI ;
  ] ;

  rr:predicateObjectMap [
    rr:predicate camt53:identifier ;
    rr:objectMap [
      rml:reference "cam:AcctSvcrRef";
    ]
 ] ;

  rr:predicateObjectMap [
    rr:predicate camt53:comment ;
    rr:objectMap [
      rml:reference "cam:AddtlNtryInf";
    ]
 ] ;

  rr:predicateObjectMap [
    rr:predicate camt53:amount ;
    rr:objectMap [
      rml:reference "cam:Amt";
      rr:datatype xsd:double ;
    ]
 ] ;

  rr:predicateObjectMap [
    rr:predicate camt53:valueDate ;
    rr:objectMap [
      rml:reference "cam:ValDt/cam:Dt";
      rr:datatype xsd:date ;
    ]
 ] ;

  rr:predicateObjectMap [
    rr:predicate camt53:bookingDate ;
    rr:objectMap [
      rml:reference "cam:BookgDt/cam:Dt";
      rr:datatype xsd:date ;
    ]
 ] ;
 .